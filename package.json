{
  "name": "cypress_example",
  "version": "1.0.0",
  "description": "QA cypressJS automation",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && \"ls cypress/integration/**/*.js\" && exit 1",
    "cy:open:dev": "./node_modules/.bin/cypress open --config-file cypress/cypress-dev.json",
    "cy:open:preprod": "./node_modules/.bin/cypress open --config-file cypress/cypress-preprod.json",
    "cy:open:automation": "./node_modules/.bin/cypress open --config-file cypress/cypress-automation.json",
    "cy:open:prod": "./node_modules/.bin/cypress open --config-file cypress/cypress-prod.json",
    "cy:clean": "[ -d \"mochawesome-report\" ] && rm -rf mochawesome-report mochawesome.json || mkdir mochawesome-report",
    "cy:run:dev": "cypress run --config-file cypress/cypress-dev.json",
    "cy:run:preprod": "cypress run --config-file cypress/cypress-preprod.json",
    "cy:run:automation": "cypress run --config-file cypress/cypress-automation.json",
    "cy:run:prod": "cypress run --config-file cypress/cypress-prod.json",
    "cy:merge-reports": "mochawesome-merge --reportDir mochawesome-report > mochawesome.json",
    "cy:generate-report": "marge mochawesome.json",
    "cy:run-with-report": "npm run cy:clean; npm run cy:run; npm run cy:merge-reports; npm run cy:generate-report",
    "cy:prettier": "prettier --check cypress/integration/**/*.js",
    "cy:prettier-fix": "prettier --write cypress/integration/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/test.git"
  },
  "keywords": [
    "cypressjs",
    "qa",
    "automation"
  ],
  "author": "QA Team",
  "license": "ISC",
  "homepage": "https://bitbucket.org/test/src/master/",
  "dependencies": {
    "axe-core": "^3.5.2",
    "cypress": "5.6.0",
    "cypress-axe": "^0.8.1",
    "cypress-downloadfile": "^1.2.0",
    "cypress-file-upload": "^4.0.7",
    "cypress-intellij-reporter": "0.0.4",
    "cypress-multi-reporters": "^1.2.4",
    "cypress-plugin-snapshots": "^1.3.0",
    "cypress-social-logins": "^1.3.0",
    "cypress-xpath": "^1.4.0",
    "debug": "^4.2.0",
    "elucidate": "0.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^4.1.0",
    "istanbul-lib-coverage": "^3.0.0",
    "lint": "^0.7.0",
    "ls": "^0.2.1",
    "minimist": "^1.2.5",
    "mocha": "^7.1.1",
    "mocha-junit-reporters": "^1.23.6",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^5.0.0",
    "mochawesome-merge": "^4.0.3",
    "mochawesome-report-generator": "^4.1.0",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "neat-csv": "^5.2.0",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5",
    "yargs": "^15.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run cy:prettier-fix"
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cypress-otp": "^1.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "moment-timezone": "^0.5.31",
    "pdf-parse": "^1.1.1",
    "pretty-quick": "^2.0.2",
    "xlsx": "^0.16.9"
  }
}
